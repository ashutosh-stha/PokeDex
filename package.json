{
  "name": "PokeDex",
  "version": "0.2.1-0",
  "private": true,
  "np": {
    "tests": "false"
  },
  "scripts": {
    "android": "react-native run-android --variant=devDebug",
    "android:qa": "react-native run-android --variant=pokedexQaDebug",
    "ios": "react-native run-ios",
    "ios:qa": "react-native run-ios --scheme='PokeDexQA",
    "lint": "eslint .",
    "start": "react-native start",
    "pod:install": "bundle exec pod install --project-directory=ios",
    "test": "jest --coverage",
    "e2e:android:build": "detox build --configuration android.emu.debug",
    "e2e:android:run": "detox test --configuration android.emu.debug",
    "e2e:ios:build": "detox build --configuration ios.sim.debug",
    "e2e:ios:run": "detox test --configuration ios.sim.debug",
    "e2e:android": "yarn e2e:android:build && yarn e2e:android:run",
    "e2e:ios": "yarn e2e:ios:build && yarn e2e:ios:run",
    "np": "np --no-publish",
    "postversion": "react-native-version"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@rematch/core": "^2.2.0",
    "axios": "^1.4.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.71.10",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-native-splash-screen": "^3.3.0",
    "react-redux": "^8.0.7",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/lodash": "^4.14.195",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "detox": "^20.9.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "np": "^8.0.4",
    "prettier": "^2.4.1",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashutosh-stha/PokeDex.git"
  }
}
